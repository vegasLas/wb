datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id          Int      @id @default(autoincrement())
  telegramId  BigInt   @unique
  chatId      String?  @unique
  username    String?
  languageCode String?
  name        String
} 